[
	{
	    "method": "POST",
	    "path": "/element/delete/brand/:id",
	    "operations": [
	        { "module": "../modules/mongo-lookup", "collection": "Sales" },
	        { "module": "validation", "schema": "sales-don't-rely-on-brand" },
	        { "module": "../modules/mongo-delete-one", "collection": "Brands", "error": "Kunne ikke slette brand" },
	        { "module": "../modules/response", "status": 200 }
	    ]
	},
	{
	    "method": "POST",
	    "path": "/element/delete/payment-option/:id",
	    "operations": [
	        { "module": "../modules/mongo-lookup", "collection": "Sales" },
	        { "module": "validation", "schema": "sales-don't-rely-on-payment-option" },
	        { "module": "../modules/mongo-update", "collection": "Sales", "query": "remove-payment-option.json" },
	        { "module": "../modules/mongo-delete-one", "collection": "PaymentOptions", "error": "Kunne ikke slette betalingsmulighed" },
	        { "module": "../modules/response", "status": 200 }
	    ]
	},
	{
	    "method": "POST",
	    "path": "/element/delete/type/:id",
	    "operations": [
	        { "module": "../modules/mongo-lookup", "collection": "Sales" },
	        { "module": "validation", "schema": "sales-don't-rely-on-type" },
	        { "module": "../modules/mongo-update", "collection": "Sales", "query": "remove-type.json" },
	        { "module": "../modules/mongo-delete-one", "collection": "Types", "error": "Kunne ikke slette type" },
	        { "module": "../modules/response", "status": 200 }
	    ]
	}
]
